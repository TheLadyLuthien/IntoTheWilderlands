{
    "schema_version": 1,
    "quilt_loader": {
        "group": "${group}",
        "id": "wilderlands",
        "version": "${version}",
        "metadata": {
            "name": "Into The Wilderlands",
            "description": "The core mod for Infinity Season 4",
            "contributors": {
                "ThatSkadoosh": "Owner"
            },
            "contact": {},
            "icon": "assets/wilderlands/icon.png",
            "license": "MIT"
        },
        "intermediate_mappings": "net.fabricmc:intermediary",
        "entrypoints": {
